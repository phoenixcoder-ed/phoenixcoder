{
  "name": "phoenixcoder",
  "version": "1.0.0",
  "description": "PhoenixCoder - AI-powered coding platform with community and enterprise versions",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/community/*",
    "apps/enterprise/*"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "test:ci": "node test-ci-cd.js",
    "test:coverage": "pnpm -r run test:coverage",
    "lint": "pnpm -r run lint",
    "lint:apps": "pnpm --filter=@phoenixcoder/community-* --filter=@phoenixcoder/enterprise-* run lint",
    "lint:shared": "pnpm --filter=@phoenixcoder/shared-* run lint",
    "format:check": "pnpm -r run format:check",
    "format:check:apps": "pnpm --filter=@phoenixcoder/community-* --filter=@phoenixcoder/enterprise-* run format:check",
    "format:check:shared": "pnpm --filter=@phoenixcoder/shared-* run format:check",
    "type-check": "pnpm -r run type-check",
    "type-check:apps": "pnpm --filter=@phoenixcoder/community-* --filter=@phoenixcoder/enterprise-* run type-check",
    "type-check:shared": "pnpm --filter=@phoenixcoder/shared-* run type-check",
    "clean": "pnpm -r run clean",
    "dev:community": "pnpm --filter=@phoenixcoder/community-* run dev",
    "dev:enterprise": "pnpm --filter=@phoenixcoder/enterprise-* run dev",
    "build:community": "pnpm --filter=@phoenixcoder/community-* run build",
    "build:enterprise": "pnpm --filter=@phoenixcoder/enterprise-* run build",
    "build:shared": "pnpm --filter=@phoenixcoder/shared-* run build"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.34.0",
    "lerna": "^8.2.3",
    "only-allow": "^1.2.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/phoenixcoder.git"
  },
  "keywords": [
    "ai",
    "coding",
    "platform",
    "monorepo",
    "typescript",
    "python",
    "fastapi",
    "react"
  ],
  "author": "PhoenixCoder Team",
  "license": "MIT",
  "dependencies": {
    "@types/nodemailer": "^6.4.19",
    "amqplib": "^0.10.8",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "js-yaml": "^4.1.0",
    "nodemailer": "^6.10.1",
    "tailwind-merge": "^2.6.0"
  }
}