{
  "type": "module",
  "name": "@phoenixcoder/community-miniapp",
  "version": "1.0.0",
  "private": true,
  "description": "Phoenix Coder的小程序端",
  "scripts": {
    "prestart": "pnpm run font-icon",
    "start": "pnpm run dev:h5",
    "build:swan": "pnpm exec taro build -- --type swan --runner vite",
    "build:alipay": "pnpm exec taro build -- --type alipay --runner vite",
    "build:tt": "pnpm exec taro build -- --type tt --runner vite",
    "build:h5": "pnpm exec taro build -- --type h5 --runner vite",
    "build:rn": "pnpm exec taro build -- --type rn --runner vite",
    "build:weapp": "pnpm exec taro build -- --type weapp --runner vite",
    "dev:weapp": "pnpm run build:weapp -- --watch",
    "dev:swan": "pnpm run build:swan -- --watch",
    "dev:alipay": "pnpm run build:alipay -- --watch",
    "dev:tt": "pnpm run build:tt -- --watch",
    "dev:h5": "pnpm run build:h5 -- --watch",
    "dev:rn": "pnpm run build:rn -- --watch",
    "font-icon": "mkdir -p src/assets/font-icons/ && icon-font-generator src/assets/svgs/**/*.svg -o src/assets/font-icons/ --csstp src/assets/icon-css-tmpl.hbs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --reporter=verbose --bail=1",
    "test:single": "vitest run --run",
    "test:silent": "vitest run --reporter=basic",
    "test:changed": "vitest run --changed",
    "test:related": "vitest run --related",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:integration": "vitest run tests/integration",
    "test:components": "vitest run tests/components",
    "test:pages": "vitest run tests/pages",
    "test:utils": "vitest run tests/utils",
    "test:all": "pnpm run test:unit && pnpm run test:integration",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss}\" \"tests/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,scss}\" \"tests/**/*.{ts,tsx,js,jsx}\"",
    "test:performance": "vitest run tests/performance --reporter=verbose",
    "test:snapshot": "vitest run --reporter=verbose --update-snapshots",
    "lint": "eslint src/ tests/ --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src/ tests/ --ext .ts,.tsx,.js,.jsx --fix"
  },
  "lint-staged": {
    "**/*.scss": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg",
      "pre-push": "./scripts/pre-push-hook.sh"
    }
  },
  "author": "smart speaking team",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@phoenixcoder/shared-miniapp": "workspace:*",
    "@tarojs/plugin-framework-react": "^4.1.5",
    "@tarojs/plugin-generator": "^4.1.5",
    "@tarojs/plugin-platform-weapp": "^4.1.5",
    "@tarojs/react": "^4.1.5",
    "@tarojs/redux": "^2.2.22",
    "@tarojs/runtime": "^4.1.5",
    "@tarojs/shared": "^4.1.5",
    "@tarojs/taro": "^4.1.5",
    "@tarojs/vite-runner": "^4.1.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.3.0",
    "redux-saga-restart": "^1.0.0",
    "seamless-immutable": "^7.1.4",
    "taro-ui": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.24.4",
    "@eslint/js": "^9.33.0",
    "@tarojs/cli": "^4.1.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.2.0",
    "@types/react": "^18.0.0",
    "@types/redux-actions": "^2.6.5",
    "@types/seamless-immutable": "^7.1.19",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-jsx-stylesheet": "^1.0.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^0.0.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "cypress": "^14.5.3",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-taro": "4.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-taro": "4.1.5",
    "happy-dom": "^12.10.3",
    "icon-font-generator": "^2.1.11",
    "jsdom": "^25.0.1",
    "msw": "^2.0.11",
    "nerv-server": "^1.5.7",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.4.2",
    "redux-actions": "^3.0.3",
    "stylelint": "^16.23.0",
    "stylelint-config-taro-rn": "4.1.5",
    "stylelint-taro-rn": "4.1.5",
    "typescript": "5.9.2",
    "validate-commit-msg": "^1.1.3",
    "vite": "^4.5.2",
    "vitest": "^2.1.8",
    "vitest-mock-extended": "^1.3.1",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "package": "com.edward",
  "value": "github:lichengwu-tw/taro-redux-typescript#master"
}
