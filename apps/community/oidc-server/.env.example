# 微信登录配置
# 警告：请替换为您的真实微信应用配置
WECHAT_APPID=your_wechat_appid_here
WECHAT_APPSECRET=your_wechat_appsecret_here
WECHAT_REDIRECT_URI=http://localhost:8001/wechat/callback

# PostgreSQL 配置
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=phoenixcoder
POSTGRES_USER=postgres
# 警告：请使用强密码，建议使用 security/generate-keys.sh 生成
POSTGRES_PASSWORD=your_strong_postgres_password_here

# Redis 配置
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0
# Redis 密码（如果启用了认证）
REDIS_PASSWORD=your_redis_password_here

# JWT 配置
# 警告：请使用强随机密钥，建议使用 security/generate-keys.sh 生成
JWT_SECRET=your_jwt_secret_key_here_at_least_32_characters
JWT_ALGORITHM=HS256

# OIDC 配置
ISSUER=http://localhost:8001