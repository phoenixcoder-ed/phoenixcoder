{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:development": "vite --mode development",
    "dev:test": "vite --mode test",
    "build": "vite build --mode prod",
    "build:test": "vite build --mode test",
    "build:prod": "vite build --mode prod",
    "serve": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "format": "prettier --write ./src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --reporter=verbose --bail=1",
    "test:single": "vitest run --run",
    "test:silent": "vitest run --reporter=basic",
    "test:changed": "vitest run --changed",
    "test:related": "vitest run --related",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:all": "pnpm run test:unit && pnpm run test:integration && pnpm run test:e2e",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,scss}\" \"tests/**/*.{ts,tsx,js,jsx}\"",
    "mock-server": "node mock-server.js",
    "dev:full": "concurrently \"pnpm run mock-server\" \"pnpm run dev\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
