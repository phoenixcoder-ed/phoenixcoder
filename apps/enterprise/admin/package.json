{
  "private": true,
  "type": "module",
  "description": "PhoenixCoder Admin Dashboard - Enterprise Edition with advanced analytics and team management",
  "scripts": {
    "dev": "vite --mode development",
    "dev:development": "vite --mode development",
    "dev:test": "vite --mode test",
    "build": "vite build --mode prod",
    "build:test": "vite build --mode test",
    "build:prod": "vite build --mode prod",
    "serve": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "format": "prettier --write ./src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "mock-server": "node mock-server.js",
    "dev:full": "concurrently \"pnpm run mock-server\" \"pnpm run dev\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@phoenixcoder/shared-components": "workspace:*",
    "@phoenixcoder/shared-services": "workspace:*",
    "@phoenixcoder/shared-types": "workspace:*",
    "@phoenixcoder/shared-utils": "workspace:*",
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.84.1",
    "autoprefixer": "^10.4.21",
    "axios": "1.11",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "react": "^19.0.0",
    "react-admin": "^5.10.0",
    "react-dom": "^19.0.0",
    "react-quill": "^2.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.1.3",
    "react-router-dom": "^7.1.3",
    "tailwind-scrollbar": "^4.0.2",
    "tailwind-scrollbar-hide": "^4.0.0",
    "tailwindcss": "^4.1.11",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.23.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.28.0",
    "vite": "^7.0.6",
    "vitest": "^2.1.9"
  },
  "name": "@phoenixcoder/enterprise-admin"
}
